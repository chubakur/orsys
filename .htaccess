RewriteEngine On

RewriteCond %{REQUEST_URI} /register$
RewriteRule .* /register.php [L]

RewriteCond %{REQUEST_URI} /auth$
RewriteRule .* /auth.php [L]

RewriteCond %{REQUEST_URI} /logout$
RewriteRule .* /logout.php [L]

RewriteCond %{REQUEST_URI} /feed$
RewriteRule .* /feed.php [L]

RewriteCond %{REQUEST_URI} /feed_new$
RewriteRule .* /feed_new.php [L]

RewriteCond %{REQUEST_URI} /feed_complete$
RewriteRule .* /feed_complete.php [L]

RewriteRule ^$ /orders.html [L]